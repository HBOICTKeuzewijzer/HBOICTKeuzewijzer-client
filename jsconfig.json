{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@public/*": [
                "public/*"
            ],
            "@pages/*": [
                "src/http/pages/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@styles/*": [
                "src/assets/styles/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ]
        }
    },
    "include": [
        "src"
    ]
}